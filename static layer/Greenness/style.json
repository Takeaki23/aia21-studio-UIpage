{
    "type": "line",
    "paint": {
        "line-color": "darkgreen",
        "line-width": 1,
        // 'line-gradient' must be specified using an expression
        // with the special 'line-progress' property
        "line-gradient": [
        "interpolate",
        ["linear"],
        ["line-progress"],
        0,
        "white",
        0.1,
        "palegreen",
        0.3,
        "lightgreen",
        0.5,
        "green",
        0.7,
        "darkolivegreen",
        1,
        "darkgreen"
        ]
        },

    "layout": {
        "line-cap": "round",
        "line-join": "round"
        }
}